{
  "name": "lobby",
  "image": "itzg/minecraft-server:latest",
  "env": {
    "TYPE": "VANILLA",
    "MINECRAFT_VERSION": "1.21.8",
    "EULA": "TRUE",
    "ONLINE_MODE": "FALSE"
  },
  "ports": {
  },
  "mounts": {
    "/var/run/docker.sock": "/var/run/docker.sock"
  },
  "labels": {
    "craftainer-resource": "true",
    "craftainer-deployment": "lobby",
    "craftainer-velocity-register": "true",
    "craftainer-velocity-fallback": "true"
  }
}
